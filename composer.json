{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"repositories": [
		{
			"type": "vcs",
			"url":	"https://github.com/herbsl/maneuver"
		},
		{
			"type": "vcs",
			"url":	"https://github.com/herbsl/Laravel-Model-Validation"
		}
	],
	"require": {
		"laravel/framework": "4.1.*",
		"fitztrev/laravel-html-minify": "dev-master",
		"intervention/image": "2.*",
		"intervention/imagecache": "2.*",
		"anahkiasen/flatten": "dev-master",
		"philf/setting": "dev-master",
		"watson/sitemap": "1.1.*",
		"fadion/maneuver": "0.1.0",
		"way/database": "0.1.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
